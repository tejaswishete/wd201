<table border = "1">
    <tr>
      <th>ID</th>
      <th>Due Date</th>
      
      <th>Title</th>
      <th>Is Complete?</th>
    </tr>
    <% for(let i=0;i<allTodos.length; i++) { %>
      <tr>
        <td><%= allTodos[i].id %></td>
        <td><%= allTodos[i].dueDate %></td>

        <td><%= allTodos[i].title %></td>
        <td><%= allTodos[i].complete %></td>
      </tr>
    <% } %>
  </table>